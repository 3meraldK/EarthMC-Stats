{
  "name": "emc-stats",
  "version": "2.4.0",
  "description": "A discord bot providing info on the EarthMC Minecraft server.",
  "license": "CC-BY-NC-SA",
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "discord.js": "14.13.0",
    "dotenv": "^16.3.1",
    "earthmc": "^8.3.0",
    "firebase": "10.3.1",
    "firebase-admin": "^11.10.1",
    "koyeb.js": "^4.2.3",
    "memory-cache": "0.2.0",
    "moment": "^2.29.4",
    "striptags": "^3.2.0",
    "tslib": "latest",
    "tsx": "latest",
    "undici": "latest"
  },
  "scripts": {
    "compile": "rimraf dist && tsc",
    "start-js": "pnpm run compile && node --gc_interval=240 --max-old-space-size=960 ./dist/index.js",
    "start": "tsx index.ts",
    "reinstall": "rimraf node_modules && pnpm install",
    "start-bun": "bun run index.ts"
  },
  "engines": {
    "node": ">=16.20.2",
    "pnpm": ">=8.6"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.49.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}