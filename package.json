{
  "name": "emc-stats",
  "version": "2.4.1",
  "description": "A discord bot providing info on the EarthMC Minecraft server.",
  "license": "CC-BY-NC-SA",
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "discord.js": "14.13.0",
    "dotenv": "^16.4.5",
    "earthmc": "^8.3.4",
    "firebase": "latest",
    "firebase-admin": "latest",
    "koyeb.js": "^4.2.3",
    "memory-cache": "0.2.0",
    "moment": "^2.29.4",
    "striptags": "^3.2.0",
    "tslib": "latest",
    "tsx": "latest",
    "undici": "5.28.4"
  },
  "scripts": {
    "compile": "rimraf dist && tsc",
    "reinstall": "rimraf node_modules && bun install",
    "start-js": "bun run compile && node ./dist/index.js",
    "start": "tsx index.ts",
    "reload": "pm2 startOrGracefulReload --attach ecosystem.json",
    "deploy": "pm2 startOrRestart ecosystem.json --env production",
    "setup": "pm2 deploy production setup"
  },
  "engines": {
    "node": ">=16.20.2",
    "pnpm": ">=8.6"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.49.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "trustedDependencies": [
    "protobufjs",
    "esbuild",
    "rimraf",
    "tsx"
  ]
}